<?xml version="1.0" encoding="utf-8"?>

<accelerator>
	<!--Rectangular collimator-->
	<module type="rectanglering" name="Collimator" medium="W700ICRU" density="1">
		<Color r="0.8" g="0.8" b="0.8" t="0.5" />
		<position unit="cm" x="0" y="0" z="-40.0" />
		<normal x="0" y="0" z="-1" />
		<xaxis x="1" y="0" z="0" />
		<size unit="cm" x1="-3" x2="3" y1="-4.8" y2="4.8" d="10" h="4.0"/>
	</module>

	<!--Фантом для снятия профиля дозы-->
	<module type="prism" name="Phantom" medium="H2O700ICRU" density="1">
		<Color r="0" g="0.4" b="0" t="0.8" />
		<position unit="cm" x="0" y="0" z="0" />
		<normal x="0" y="0" z="1" />
		<xaxis x="1" y="0" z="0" />
		<size unit="cm" ax="60" ay="60" az="40"/>
	</module>
</accelerator>
